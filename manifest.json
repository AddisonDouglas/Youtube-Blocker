{
    "manifest_version": 3,
    "name": "Youtube Eater V 1.1",
    "description": "Help reduce how much time you waste on yotube",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "hi.png"
      },
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["myscript.js"]
        }
    ]

  }